{
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        { "open": "{", "close": "}" },
        { "open": "[", "close": "]" },
        { "open": "(", "close": ")" },
        { "open": "\"", "close": "\"" },
        { "open": "'", "close": "'" }
    ],
    "surroundingPairs": [
        { "open": "{", "close": "}" },
        { "open": "[", "close": "]" },
        { "open": "(", "close": ")" },
        { "open": "\"", "close": "\"" },
        { "open": "'", "close": "'" }
    ],
    "indentationRules": {
        "increaseIndentPattern": "\\b(Algo|Begin|function|then|else)\\b|\\bwhile\\b\\s*\\([^\\)]*\\)|^\\s*for\\b.*",
        "decreaseIndentPattern": "\\bEnd\\b"
    }
}