{
    "name": "DAUPS-DOCS",
    "scopeName": "source.daups-docs",
    "patterns": [
        {
            "name": "support.type.builtin.daups-docs",
            "match": "\\b(int|float|bool|str|array|of|type(?:_[a-zA-Z0-9]+)?)\\b"
        },
        {
            "include": "#keywords"
        }
    ],
    "repository": {
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.control.daups-docs",
                    "match": "\\b(Algo|Begin|End|function)\\b"
                },
                {
                    "name": "meta.structure.block-label.daups-docs",
                    "match": "\\b(Instructions|Condition|DÃ©claration_de_variables|Variable_declarations)\\b"
                },
                {
                    "name": "keyword.control.flow.daups-docs",
                    "match": "\\b(if|then|else|for|while|to|downto|return|or|and)\\b"
                },
                {
                    "name": "constant.numeric.daups-docs",
                    "match": "\\b\\d+(\\.\\d+)?\\b"
                },
                {
                    "name": "string.quoted.double.daups-docs",
                    "match": "\"([^\"\\\\]|\\\\.)*\""
                },
                {
                    "name": "string.quoted.single.daups-docs",
                    "match": "'([^'\\\\]|\\\\.)*'"
                },
                {
                    "name": "comment.line.number-sign.daups-docs",
                    "match": "#.*$"
                },
                {
                    "name": "support.function.builtin.daups-docs",
                    "match": "\\b(create_array|print|get|run|SQRT|nombreAleatoire|size)\\b"
                },
                {
                    "name": "entity.name.function.daups-docs",
                    "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\s*(?=\\()"
                },
                {
                    "name": "variable.other.daups-docs",
                    "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
                },
                {
                    "name": "punctuation.daups-docs",
                    "match": "[()]|[\\[\\]]|[{}]"
                }
            ]
        }
    }
}